{
	"name": "k-scheme",
	"displayName": "K-Scheme",
	"description": "Extension for Korean Scheme",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onCommand:k-scheme.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"languages": [
			{
				"id": "scheme",
				"aliases": [
					"Scheme",
					"scheme"
				],
				"extensions": [
					".scm",
					".ss",
					".sch",
					".rkt"
				],
				"configuration": "./scheme.configuration.json"
			}
		],
		"snippets": [
			{
				"language": "scheme",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}